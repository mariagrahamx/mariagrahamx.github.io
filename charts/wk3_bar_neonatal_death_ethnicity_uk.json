{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "Bar chart with text labels. Set domain to make the frame cover the labels.",
  "data": {
    "url": "https://raw.githubusercontent.com/mariagrahamx/mariagrahamx.github.io/refs/heads/main/wk3/wk3_neonatal_mortality_data_2011_2017.csv"
  },
  "title": {
    "text": "Relative Risk Of Neonatal Death by Mothers' Ethnic Group (England & Wales, 2011â€“2017)",
     "anchor": "start"
  },
  "height": 300,
  "width": 400,
  "layer": [
    {
      "mark": "bar",
      "encoding": {
        "y": {
          "field": "Group",
          "type": "nominal",
          "sort": "-x",
          "axis": {
            "title": "Mother's Ethnic Group",
            "labelLimit": 0,
            "titlePadding": 40
          }
        },
        "x": {
          "field": "Estimate",
          "type": "quantitative",
          "axis": {"title": "Relative Risk Of Neonatal Death vs White British "}
        },
        "color": {"field": "Group", "type": "nominal", "legend": null}
      }
    },
    {
      "mark": {"type": "errorbar", "ticks": true},
      "encoding": {
        "y": {"field": "Group", "type": "nominal"},
        "x": {
          "field": "Lower confidence interval",
          "type": "quantitative",
          "title": ""
        },
        "x2": {"field": "Upper confidence interval"}
      }
    }
  ],
  "config": {}
}