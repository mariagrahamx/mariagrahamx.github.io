{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Ethiopia Unemployment",
    "anchor": "start",
    "subtitle": [
      "Share Of The Labour Force Without Work",
      "Source: CSA (WB) through ECO API"
    ],
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#126cd3",
    "subtitleColor": "#616161"
  },
  "height": 250,
  "width": 300,
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/eth/unem?vega",
    "format": {"type": "json"}
  },
  "mark": {
    "type": "line",
    "point": {"color": "#620392b9", "filled": false, "fill": "white"}
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "Year", "titleColor": "#126cd3", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "Unemployment Rate (%)",
        "titleColor": "#126cd3",
        "grid": false
      }
    }
  },
  "config": {}
}